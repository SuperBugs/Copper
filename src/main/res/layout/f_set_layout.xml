                        android:layout_height="match_parent"
                        android:layout_marginRight="@dimen/f_set_right_m"
                        android:gravity="center"
                        android:text="@string/f_seting_share" />
                </LinearLayout>

            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/little_gray" />

            <LinearLayout
                android:id="@+id/f_set_help"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="@drawable/bg_set"
                android:orientation="vertical">

                <LinearLayout

                    android:layout_width="match_parent"
                    android:layout_height="@dimen/f_set_h"
                    android:background="@drawable/set_item_selector"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="@dimen/f_set_item_h"
                        android:layout_height="@dimen/f_set_item_h"
